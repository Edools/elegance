<!-- Matomo -->
<script type="text/javascript">
  var element = document.getElementById('school-header');
  var attributeContent = element.getAttribute('data-user');
  var object = JSON.parse(attributeContent);
  /*var datauser = document.getElementById('').getAttribute('data-user');*/
  var _paq = window._paq || [];
  /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
  _paq.push(['setUserId', object.email]);
  _paq.push(['setCustomVariable',
    // Index, the number from 1 to 5 where this custom variable name is stored
    1,
    // Name, the name of the variable, for example: Gender, VisitorType
    "Email",
    // Value, for example: "Male", "Female" or "new", "engaged", "customer"
    object.email,
    // Scope of the custom variable, "visit" means the custom variable applies to the current visit
    "visit"
   ]);
    _paq.push(['setCustomVariable',
    // Index, the number from 1 to 5 where this custom variable name is stored
    2,
    // Name, the name of the variable, for example: Gender, VisitorType
    "Nome",
    // Value, for example: "Male", "Female" or "new", "engaged", "customer"
    object.name,
    // Scope of the custom variable, "visit" means the custom variable applies to the current visit
    "visit"
   ]);
   _paq.push(['setCustomVariable',
    // Index, the number from 1 to 5 where this custom variable name is stored
    3,
    // Name, the name of the variable, for example: Gender, VisitorType
    "CPF",
    // Value, for example: "Male", "Female" or "new", "engaged", "customer"
    object.cpf,
    // Scope of the custom variable, "visit" means the custom variable applies to the current visit
    "visit"
   ]);
   _paq.push(['setCustomVariable',
    // Index, the number from 1 to 5 where this custom variable name is stored
    4,
    // Name, the name of the variable, for example: Gender, VisitorType
    "ID",
    // Value, for example: "Male", "Female" or "new", "engaged", "customer"
    object.id,
    // Scope of the custom variable, "visit" means the custom variable applies to the current visit
    "visit"
   ]);
  _paq.push(['trackPageView']);
  _paq.push(['enableLinkTracking']);
  /*console.log(object.id);
  console.log(object.email);
  console.log(object.name);*/
  (function() {
    var u="//sx22.ricodomingues.com.br/";
    _paq.push(['setTrackerUrl', u+'matomo.php']);
    _paq.push(['setSiteId', '2']);
    var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
    g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
  })();
</script>
<!-- End Matomo Code -->

</body>
